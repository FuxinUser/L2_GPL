    <!--Socket Port-->
	  
    <add key="LocalIP" value="127.0.0.1" />
    <add key="LocalPort" value="9105" />	  
    
	  <add key="RemoteIP" value="127.0.0.1" />
    <add key="RemotePort" value="7788" />
	
	  <add key="CyclePlcRcvLocalIP" value="127.0.0.1" />
